<html lang="en">
<!-- CSS only -->

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>CRUD</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.1/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-iYQeCzEYFbKjA/T2uDLTpkwGzCiq6soy8tYaI1GyVh/UjpbCx/TYkiZhlZB6+fzT" crossorigin="anonymous">
</head>

<body>
    <div class="container">
        <form action="/put-crud" method="POST" class="mt-5">
            <h4 class="font mb-5">Edit user's infomation</h4>
            <div class="row">
                <div class="col-12 mb-3">
                    <input class="form-control" type="text" value="<%=dataEdit.id %>" hidden>
                    <input class="form-control" type="text" value="<%=dataEdit.email %>" disabled>
                </div>
                <div class="col-md-6 mb-3">
                    <label for="firstname" class="form-label">First name</label>
                    <input type="text" class="form-control" id="firstname" name="firstname"
                        value="<%=dataEdit.firstName %>">
                </div>
                <div class="col-md-6 mb-3">
                    <label for="lastname" class="form-label">Last name</label>
                    <input type="text" class="form-control" id="lastname" name="lastname"
                        value="<%=dataEdit.lastName %>">
                </div>
                <div class="col-12 mb-3">
                    <label for="address" class="form-label">Address</label>
                    <input type="text" class="form-control" id="address" placeholder="address" name="address"
                        value="<%=dataEdit.address %>">
                </div>
                <div class="col-12 mb-3">
                    <button type="submit" class="btn btn-primary">Update</button>
                </div>
            </div>
        </form>
    </div>
</body>
